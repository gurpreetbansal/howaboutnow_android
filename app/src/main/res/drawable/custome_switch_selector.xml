<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <gradient android:angle="270" android:endColor="@android:color/white" android:startColor="@android:color/white" />
            <corners android:radius="@dimen/_15sdp" />
            <size android:width="@dimen/_20sdp" android:height="@dimen/_20sdp" />
            <stroke android:width="@dimen/_2sdp" android:color="@android:color/white" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <gradient android:angle="270" android:endColor="@android:color/white" android:startColor="@android:color/white" />
            <corners android:radius="@dimen/_15sdp" />
            <size android:width="@dimen/_20sdp" android:height="@dimen/_20sdp" />
            <stroke android:width="@dimen/_2sdp" android:color="@android:color/white" />
        </shape>
    </item>
</selector>
